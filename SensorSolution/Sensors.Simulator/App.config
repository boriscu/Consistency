<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.serviceModel>
		<client>
			<!-- Matches the host endpoint at net.tcp://localhost:9001/SensorService -->
			<endpoint address="net.tcp://localhost:9001/SensorService"
					  binding="netTcpBinding"
					  contract="Sensors.Contracts.ISensorService" />
		</client>
	</system.serviceModel>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
</configuration>
